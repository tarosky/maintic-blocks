	"apiVersion": 3,
	"name": "maintic/faq-question",
	"version": "1.0.0",
	"title": "質問",
	"category": "maintic",
	"icon": "editor-help",
	"description": "FAQの質問部分",
	"parent": ["maintic/faq-item"],
	"attributes": {
		"question": {
			"type": "string",
