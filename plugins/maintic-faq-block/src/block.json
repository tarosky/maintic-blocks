{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "maintic/faq",
	"version": "1.0.0",
	"title": "よくある質問",
	"category": "maintic",
	"icon": "editor-help",
	"description": "よくある質問（FAQ）を表示するブロック。アコーディオン形式と常時展開形式を選択できます。",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "accordion"
		}
	},
	"providesContext": {
		"maintic/faq/variant": "variant"
	},
	"supports": {
		"align": true,
		"color": {
			"background": true,
			"text": true,
			"link": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"allowedBlocks": ["maintic/faq-item"],
	"textdomain": "maintic-faq-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
